@model NezamEquipment.Web.Areas.Admin.Features.Employees.AdminNezamEmployeAddViewModel
@{
    ViewBag.PageTitle = "افزودن کارمند جدید";
}

<div class="col-md-12">
    @Html.PageTitle()
</div>
<div class="col-md-12" id="form">
    @using (Ajax.BeginForm("Add", AjaxOptionsExtension.Get(url: Url.Action(MVC.Admin.Employees.Add()))))
    {
        Html.RenderPartial("_Form", Model.PartialForm);
    }
</div>

@section InBodyTag
{
    <script type="text/javascript">
        GoToPageOnSuccessUrl = '@Url.Action(MVC.Admin.Employees.Index())';
    </script>
}
